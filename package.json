{
  "name": "edb-parcel",
  "version": "0.4.0",
  "description": "Exemplo de uso do parcel-bundler",
  "main": "index.js",
  "directories": {
    "doc": "docs"
  },
  "dependencies": {},
  "devDependencies": {
    "cross-var-no-babel": "^1.2.0",
    "http-server": "^0.11.1",
    "node-sass": "^4.9.2",
    "parcel-bundler": "^1.9.7",
    "parcel-plugin-clean-dist": "0.0.6",
    "standard": "^11.0.1"
  },
  "scripts": {
    "dev": "parcel src/index.html -d ./docs -p 8080 --no-cache",
    "build": "parcel build src/index.html -d ./docs --public-url ./ --no-source-maps --no-cache",
    "prod": "http-server ./docs -o"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/digitalbocca/edb-parcel.git"
  },
  "keywords": [
    "parcel",
    "bundler",
    "edb",
    "exemplo",
    "javascript"
  ],
